/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=b, out=notb);
    Not(in=a, out=nota);
    And(a=a, b=notb, out=andout1);
    And(a=b, b=nota, out=andout2);
    Or(a=andout1, b=andout2, out=out);
}
